		</physical>
		<material name="steel" />
		<look name="red" />
		<world_transform xyz="-2.5 -11.5 1.7" rpy="0.0 3.14 0.0" />
	</static>

	<static name="bucket" type="model" collision="false">
		<physical>
			<mesh filename="meshes/tub.obj" scale="1.0" />
			<origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0" />
