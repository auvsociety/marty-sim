<?xml version="1.0"?>
<scenario>
	<static name="Floor" type="plane">
		<material name="Neutral"/>
		<look name="pool"/>
		<world_transform xyz="0 0 2.0" rpy="0.0 0.0 0.0"/>
	</static>
	<!-- may vary the x from -15 to 25 -->
	<static name="task_gate" type="model" collision="false">
		<physical>
			<mesh filename="meshes/task_gate.obj" scale="1.0" />
			<origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0" />
		</physical>
		<material name="steel" />
		<look name="Yellow" />
		<world_transform xyz="$(arg task_gate_coord)" rpy="0.0 3.14 0.0" />
	</static>

	<!-- may vary the y axis from -4.5 to -8.5 -->
	<!-- may vary the x from -15 to 25 -->
	<dynamic name="flare" type="cylinder" physics="surface">
		<dimensions radius="0.075" height="1.5"/>
		<origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
		<material name="Steel"/>
		<look name="orange"/>
		<world_transform xyz="$(arg flare_coord)" rpy="0.0 0.0 0.0"/>
	</dynamic>

	<static name="bucket" type="model" collision="false">
		<physical>
			<mesh filename="meshes/tub.obj" scale="1.0" />
